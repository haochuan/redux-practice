    "express-http-proxy": "^0.7.2",
    "isomorphic-fetch": "*",
    "keymirror": "*",
    "lodash": "*",
    "morgan": "*",
    "node-basicauth": "*",
    "react": "*",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "*",
    "react-router": "*",
    "redux": "*",
    "redux-logger": "*",
    "redux-thunk": "^2.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "devDependencies": {
